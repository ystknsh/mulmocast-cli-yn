{
  "name": "mulmo",
  "version": "0.0.1",
  "description": "",
  "main": "lib/cli.js",
  "bin": {
    "mulmo": "lib/cli.js"
  },
  "files": [
    "./lib"
  ],
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "gen": "npx tsx ./src/audio.ts",
    "ja": "npx tsx ./src/translate.ts",
    "mov": "npx tsx ./src/movie.ts",
    "images": "npx tsx ./src/images.ts",
    "imagep": "npx tsx ./src/imagep.ts",
    "split": "npx tsx ./src/split.ts",
    "fixtext": "npx tsx ./src/fixtext.ts",
    "movie": "npx tsx ./src/movie.ts",
    "test": "rm -f scratchpad/test*.* && npx tsx ./src/audio.ts scripts/test/test.json && npx tsx ./src/images.ts scripts/test/test.json && npx tsx ./src/movie.ts scripts/test/test.json",
    "ci_test": "node --test  --require ts-node/register ./test/*/test_*.ts",
    "lint": "eslint src",
    "build": "tsc",
    "prompt": "npx ts-node ./src/tools/prompt.ts",
    "cli": "npx ts-node ./src/cli.ts",
    "latest": "yarn upgrade-interactive  --latest",
    "format": "prettier --write '{src,scripts,draft,ideason,scripts_mag2,proto,test,graphai,output}/**/*.{ts,json}'"
  },
  "repository": "git+ssh://git@github.com/receptron/mulmocast-cli.git",
  "author": "snakajima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/receptron/mulmocast-cli/issues"
  },
  "homepage": "https://github.com/receptron/mulmocast-cli#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@graphai/agents": "^1.0.2",
    "@graphai/tts_nijivoice_agent": "^0.0.2",
    "@graphai/tts_openai_agent": "^0.0.2",
    "@graphai/vanilla_node_agents": "^1.0.1",
    "@types/fluent-ffmpeg": "^2.1.26",
    "canvas": "^3.1.0",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^9.15.1",
    "graphai": "^1.0.5",
    "openai": "^4.67.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.33",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
